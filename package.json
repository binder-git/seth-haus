{
  "name": "seth-haus",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Seth's Triathlon Haus - An ecommerce prototype powered by Commerce Layer and Netlify",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "netlify dev",
    "dev:ui": "cd ui && npm run dev",
    "dev:functions": "cd netlify/functions && npm run dev",
    "build": "cd ui && npm run build && cd ../netlify/functions && npm run build",
    "build:ui": "cd ui && npm run build",
    "build:functions": "cd netlify/functions && npm run build",
    "start": "netlify dev"
  },
  "dependencies": {
    "@commercelayer/sdk": "^6.41.0"
  },
  "devDependencies": {
    "@netlify/functions": "^3.1.8",
    "@types/node": "^20.17.47",
    "netlify-cli": "^17.0.0",
    "typescript": "^5.1.3"
  }
}
