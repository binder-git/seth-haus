{
  "name": "seth-haus",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Seth's Triathlon Haus - E-commerce site with Commerce Layer",
  "workspaces": [
    "ui",
    "netlify/functions"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "netlify dev",
    "dev:ui": "npm run dev --workspace=ui",
    "dev:functions": "npm run dev --workspace=netlify/functions",
    "build": "npm run build --workspace=ui && npm run build --workspace=netlify/functions",
    "build:ui": "npm run build --workspace=ui",
    "build:functions": "npm run build --workspace=netlify/functions",
    "start": "netlify dev"
  },
  "dependencies": {
    "@commercelayer/sdk": "^6.41.0"
  },
  "devDependencies": {
    "@netlify/functions": "^3.1.8",
    "@types/node": "^20.17.47",
    "netlify-cli": "^17.0.0",
    "typescript": "^5.1.3"
  }
}