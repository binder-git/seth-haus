{
  "name": "seth-haus",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Seth's Triathlon Haus - E-commerce site with Commerce Layer",
  "workspaces": [
    "ui",
    "netlify/functions"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "netlify dev --live --port 8888",
    "dev:ui": "cd ui && npm run dev",
    "build": "npm run build:ui && npm run build:functions",
    "build:ui": "cd ui && npm run build",
    "build:functions": "cd netlify/functions && npm run build",
    "start": "netlify dev"
  },
  "dependencies": {
    "@commercelayer/sdk": "^6.41.0"
  },
  "devDependencies": {
    "@netlify/functions": "^3.1.8",
    "@types/node": "^20.17.47",
    "netlify-cli": "^17.0.0",
    "typescript": "^5.1.3"
  }
}
