{
  "name": "seth-haus",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Seth's Triathlon Haus - An ecommerce prototype powered by Commerce Layer and Netlify",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "netlify dev",
    "dev:ui": "cd ui && npm run dev",
    "dev:functions": "cd netlify/functions && npm run dev",
    "build": "cd ui && npm run build && cd ../netlify/functions && npm run build",
    "build:ui": "cd ui && npm run build",
    "build:functions": "cd netlify/functions && npm run build",
    "start": "netlify dev"
  },
  "dependencies": {
    "@netlify/functions": "^3.1.9",
    "@types/jest": "^29.5.14",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "node-fetch": "^2.7.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^20.17.50",
    "netlify-cli": "^17.0.0"
  }
}
