# Commerce Layer Configuration
COMMERCE_LAYER_CLIENT_ID=your_client_id_here
COMMERCE_LAYER_CLIENT_SECRET=your_client_secret_here
COMMERCE_LAYER_ORGANIZATION=your_organization_slug
COMMERCE_LAYER_DOMAIN=commercelayer.io

# Market Configuration
# EU Market
EU_MARKET_SCOPE=your_eu_scope_id
EU_SKU_LIST_ID=your_eu_sku_list_id

# UK Market
UK_MARKET_SCOPE=your_uk_scope_id
UK_SKU_LIST_ID=your_uk_sku_list_id

# Development Settings
# Set to 'development' for more verbose logging
NODE_ENV=production

# Local Development
# Set this to your local development server URL (e.g., http://localhost:9999)
NETLIFY_DEV_URL=http://localhost:9999
